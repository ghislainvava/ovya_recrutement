  <div class=\"form-group m-5\">\n              <label for=\"InputName\"></label>\n                <input name=\"InputName\" \n                       type=\"text\" \n                       class=\"form-control\" \n                       id=\"InName\" \n                       v-model=\"InName\" \n                       placeholder=\"Entrez votre nom\"  \n                       required>\n          </div>\n         \n          <div class=\"form-group m-5\">\n                <label for=\"InputEmail\"></label>\n                <input  name=\"InputEmail\" \n                        type=\"email\" \n                        class=\"form-control\" \n                        id=\"InEmail\"  \n                        v-model=\"InEmail\" \n                        aria-describedby=\"emailHelp\" \n                        placeholder=\"Entrez  l'email du client\" \n                        required>\n          </div>\n\n           <div class=\"form-group\">\n\n            <label for=\"date_start\">date : </label>\n\n          <input type=\"text\" id=\"date_start\" name=\"date_start\"\n                v-model=\"date_start\">\n\n         </div> \n       \n\n         <div class=\"form-group\">\n\n            <label for=\"timedate\">date : </label>\n\n          <input type=\"text\" id=\"dtime\" name=\"dtime\"\n                v-model=\"date_end\">\n\n         </div> \n\n         <div class=\"form-group m-5\">\n                <label for=\"InputEmail\"></label>\n                <input  name=\"InputEmail\" \n                        type=\"email\" \n                        class=\"form-control\" \n                        id=\"InputEmail\"  \n                        v-model=\"InputEmail\" \n                        aria-describedby=\"emailHelp\" \n                        placeholder=\"Entrez votre email\" \n                        required>\n          </div>\n         \n          <button type=\"submit\" \n                  class=\"btn btn-secondary mt-3\"\n                  >S'inscrire</button>\n    </form> \n    \n  </div>\n  \n  \n</template>\n\n\n<script>\nimport axios from 'axios'  //client http\n\n\nexport default {\n  name: 'App',\n  data () {\n      return {\n       InEmail:\"\",\n       InName:\"\",\n       date_end:\"\",\n       InputEmail:\"\",\n       date_start:\"\",\n      \n      }\n    },\n          methods: {\n          async   handSubmit(){               //methode pour envoyer l'inscription sur le serveur\n            await axios.post('http://localhost:3000/api/user/signup', { \n                'InEmail' : this.InEmail,\n                'InName' : this.IName,\n                'date_start': this.date_start,\n                'InputEmail': this.InputEmail,\n                'date_end':this.date_end\n              })\n                .then(() => {\n                  alert(\"Votre compte a bien été crée !\")\n                  this.$router.push('/');\n                })\n                .catch(() => {\n                  \n                  alert(\"Votre compte n'a pu être créer car le pseudo ou l'email est déjà utilisé\")\n                  \n                })\n              }\n        },\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n\n</style>\n"]}]}