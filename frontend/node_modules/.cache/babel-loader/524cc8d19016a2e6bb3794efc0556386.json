AA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACZ,KAAK,CAAC,MAAN,kDAAuD,MAAI,CAAC,WAA5D,GACL,IADK,CACA,YAAM;AACV,kBAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,GAAlB;AACD,iBAHK,EAIL,KAJK,CAIC,UAAC,KAAD,EAAW;AACd,sBAAM,MAAK,GAAI,KAAK,CAAC,GAAN,CAAU,IAAzB;AACA,kBAAA,KAAK,CAAC,MAAM,CAAC,KAAR,CAAL;AACC,iBAPC,CADY;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWpB;AA9BO;AAbJ,CAAf","sourcesContent":["<template>\n  <div>\n\n    <img alt=\"logo\" src=\"./assets/Immobilier.jpg\">\n    <h1>L'agence Immobiliere</h1>\n\n    <form  @submit.prevent=\"handSubmit\"  class=\"form-group m-3 container\"> <!-- formulaire -->\n         \n         <div  class=\"row justify-content-md-center\"> \n            <div class=\"col-md-6 \">\n                <div class=\"form-group m-5\">\n                      <label for=\"InEmail\"></label>\n                      <input  name=\"InEmail\" \n                              type=\"email\" \n                              class=\"form-control\" \n                              id=\"InEmail\"  \n                              v-model=\"InEmail\" \n                              aria-describedby=\"emailHelp\" \n                              placeholder=\"Entrez  votre email\" \n                              required>\n                </div>\n\n                <div class=\"form-group m-5\">\n                      <label for=\"InputEmail\"></label>\n                      <input  name=\"InputEmail\" \n                              type=\"email\" \n                              class=\"form-control\" \n                              id=\"InputEmail\"  \n                              v-model=\"InputEmail\" \n                              aria-describedby=\"emailHelp\" \n                              placeholder=\"Entrez  l'email du client\" \n                              required>\n                </div>\n\n          \n            </div>\n\n              <div class=\"col-md-6 mt-5 justify-content-end\">\n                    <div class=\"form-group mt-5 d-flex \">\n\n                          <label for=\"date_start\">date début de visite : </label>\n\n                          <input type=\"text\"\n                          placeholder=\"03-31-2021\"\n                          id=\"date_start\" \n                          name=\"date_start\"\n                          class=\"ml-2\"\n                          v-model=\"date_start\"\n                          required>\n\n                  </div> \n                \n\n                  <div class=\"form-group mt-5 d-flex \">\n\n                          <label for=\"timedate\">date de fin de visite : </label>\n\n                          <input type=\"text\" \n                            placeholder=\"03-31-2021\"\n                            id=\"dtime\" \n                            name=\"dtime\"\n                            class=\"ml-2\"\n                            v-model=\"date_end\"\n                            required>\n\n                  </div> \n            </div>\n         </div>\n          <button type=\"submit\" \n                  class=\"btn btn-secondary mt-3\"\n                  >Envoyer\n          </button>\n    </form> \n    \n  <form  @submit.prevent=\"handsup\"  class=\"form-group m-3 \">\n\n        <div class=\"form-group m-5\">\n                      <label for=\"Emailclient\">Supprimer un client</label>\n                      <input  name=\"Emailclient\" \n                              type=\"email\" \n                              class=\"form-control\" \n                              id=\"Emailclient\"  \n                              v-model=\"Emailclient\" \n                              aria-describedby=\"emailHelp\" \n                              placeholder=\"Entrez  l'email du client\" \n                              required>\n          </div>\n\n      <button @click=\"deleteSubmit()\"\n                  class=\"btn btn-secondary mt-3\"\n                  >Supprimer l'acquéreur     <i class=\"fas fa-trash\"></i>\n      </button>\n  </form>\n\n  </div>\n  \n  \n</template>\n\n\n<script>\nimport axios from 'axios'  //client http\n\n\nexport default {\n  name: 'App',\n  data () {\n      return {\n       InEmail:\"\",\n       InputEmail:\"\",\n       date_end:\"\",\n       ccial_id:\"\",\n       date_start:\"\",\n       Emailclient:\"\"\n      \n      }\n    },\n          methods: {\n          async   handSubmit(){               //methode pour envoyer l'inscription sur le serveur\n            await axios.post('http://localhost:3000/api/visite', { \n                'InEmail':this.InEmail,\n                'InputEmail': this.InputEmail,\n                'date_start': this.date_start,\n                'date_end':this.date_end\n              })\n                .then(() => {\n                  alert(\"Votre compte a bien été crée !\")\n                  this.$router.push('/');\n                })\n                .catch(() => {\n                  \n                  alert(\"Votre compte n'a pu être créer car le pseudo ou l'email est déjà utilisé\")\n                  \n                })\n              },\n            \n            async deleteSubmit(){\n              await axios.delete(`http://localhost:3000/api/visite?email=${this.Emailclient}`)\n              .then(() => {\n                this.$router.push('/');\n              })\n              .catch((error) => {\n                  const erreur = error.res.data\n                  alert(erreur.error)\n                  });\n                \n             \n            }\n        },\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n\n</style>\n"],"sourceRoot":""}]}